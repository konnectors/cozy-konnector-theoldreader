{
  "name": "cozy-konnector-theoldreader",
  "version": "1.6.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:konnectors/cozy-konnector-theoldreader.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build",
    "data"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "tsc && node ./src/index.js",
    "dev": "tsc && cozy-konnector-dev",
    "standalone": "tsc && cozy-konnector-standalone",
    "pretest": "npm run clean",
    "clean": "rm -rf ./data",
    "build": "tsc && webpack",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-$npm_package_repository_url}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "5.12.1",
    "pdfjs": "2.5.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.42",
    "@types/cheerio": "0.22.35",
    "@types/moment": "2.13.0",
    "copy-webpack-plugin": "12.0.2",
    "cozy-app-publish": "0.41.0",
    "cozy-jobs-cli": "2.4.4",
    "eslint-config-cozy-app": "2.1.1",
    "git-directory-deploy": "1.5.1",
    "husky": "9.1.7",
    "svgo": "3.3.2",
    "ts-loader": "9.5.2",
    "typescript": "5.8.2",
    "webpack": "5.97.1",
    "webpack-cli": "4.10.0"
  }
}
