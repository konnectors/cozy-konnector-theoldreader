{
  "name": "cozy-konnector-theoldreader",
  "version": "1.4.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:konnectors/cozy-konnector-theoldreader.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build",
    "data"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "tsc && node ./src/index.js",
    "dev": "tsc && cozy-konnector-dev",
    "standalone": "tsc && cozy-konnector-standalone",
    "pretest": "npm run clean",
    "clean": "rm -rf ./data",
    "build": "tsc && webpack",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-$npm_package_repository_url}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.42.0",
    "pdfjs": "2.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.33",
    "@types/cheerio": "0.22.28",
    "@types/moment": "2.13.0",
    "copy-webpack-plugin": "6.4.1",
    "cozy-app-publish": "0.27.0",
    "cozy-jobs-cli": "1.16.0",
    "eslint-config-cozy-app": "2.1.0",
    "git-directory-deploy": "1.5.1",
    "husky": "4.3.8",
    "svgo": "1.3.2",
    "ts-loader": "8.2.0",
    "typescript": "4.0.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  }
}
