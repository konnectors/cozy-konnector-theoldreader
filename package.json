{
  "name": "cozy-konnector-theoldreader",
  "version": "1.3.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-theoldreader.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "tsc && node ./src/index.js",
    "dev": "tsc && cozy-run-dev",
    "standalone": "tsc && cozy-run-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-theoldreader.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.12.1",
    "pdfjs": "2.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.25",
    "@types/cheerio": "0.22.10",
    "@types/moment": "2.13.0",
    "copy-webpack-plugin": "4.6.0",
    "cozy-app-publish": "0.9.1",
    "cozy-jobs-cli": "1.6.1",
    "eslint": "5.12.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-config-cozy-app": "1.1.7",
    "git-directory-deploy": "1.5.1",
    "husky": "1.3.1",
    "konitor": "0.10.1",
    "ts-loader": "5.3.3",
    "typescript": "3.2.2",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0"
  }
}
