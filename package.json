{
  "name": "cozy-konnector-theoldreader",
  "version": "1.4.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-theoldreader.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "scripts": {
    "start": "tsc && node ./src/index.js",
    "dev": "tsc && cozy-konnector-dev",
    "standalone": "tsc && cozy-konnector-standalone",
    "pretest": "npm run clean",
    "test": "konitor testit .",
    "check": "konitor check .",
    "clean": "rm -rf ./data",
    "build": "tsc && webpack",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-git@github.com:konnectors/cozy-konnector-theoldreader.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.34.5",
    "pdfjs": "2.4.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.32",
    "@types/cheerio": "0.22.21",
    "@types/moment": "2.13.0",
    "copy-webpack-plugin": "6.0.3",
    "cozy-app-publish": "0.23.1",
    "cozy-jobs-cli": "1.13.6",
    "eslint-config-cozy-app": "1.5.0",
    "git-directory-deploy": "1.5.1",
    "husky": "4.2.5",
    "svgo": "1.3.2",
    "ts-loader": "8.0.1",
    "typescript": "3.9.7",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  }
}
