{
  "name": "cozy-konnector-theoldreader",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-theoldreader.git"
  },
  "keywords": [],
  "author": "laedit",
  "license": "AGPL-3.0",
  "main": "./src/index.js",
  "eslintConfig": {
    "extends": [
      "eslint-config-cozy-app"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "tsc && node ./src/index.js",
    "dev": "tsc && cozy-run-dev",
    "standalone": "tsc && cozy-run-standalone",
    "test": "konitor testit .",
    "check": "konitor check .",
    "build": "webpack",
    "precommit": "yarn lint",
    "lint": "eslint --fix .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-theoldreader.git}",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "travisDeployKey": "./bin/generate_travis_deploy_key"
  },
  "dependencies": {
    "cozy-konnector-libs": "4.3.0",
    "pdfjs": "2.0.0-alpha.8"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/cheerio": "0.22.7",
    "@types/moment": "2.13.0",
    "copy-webpack-plugin": "4.5.1",
    "cozy-app-publish": "0.3.6",
    "cozy-jobs-cli": "1.2.0",
    "eslint": "4.19.1",
    "eslint-config-cozy-app": "0.8.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "konitor": "0.8.3",
    "ts-loader": "4.3.0",
    "typescript": "2.8.3",
    "webpack": "4.9.1",
    "webpack-cli": "2.1.4"
  }
}
